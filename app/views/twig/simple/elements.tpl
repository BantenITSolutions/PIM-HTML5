{% extends "layouts/simple.tpl" %}

{% block simple_content %}
	<h1>New Elements</h1>
	<table class="table table-hover">
          <thead>
            <tr>
              <th>Tag</th>
              <th>Description</th>
            </tr>
          </thead>
          <tbody>
          	<tr>
          		<td><code>&lt;article&gt;</code></td>
          		<td>Mendefinisikan artikel</td>
          	</tr>

			<tr>
          		<td><code>&lt;aside&gt;</code></td>
          		<td>Mendefinisikan isi di sebelah konten halaman</td>
          	</tr>

			<tr>
          		<td><code>&lt;audio&gt;</code></td>
          		<td>Mendefinisikan konten audio/suara</td>
          	</tr>

			<tr>
          		<td><code>&lt;bdi&gt;</code></td>
          		<td>Mengisolasi bagian dari teks yang mungkin memiliki format yang berbeda dari teks lain diluarnya</td>
          	</tr>

			<tr>
          		<td><code>&lt;canvas&gt;</code></td>
          		<td>Digunakan untuk menggambar grafik, saat runtime, melalui scripting (biasanya Javascript)</td>
          	</tr>

			<tr>
          		<td><code>&lt;command&gt;</code></td>
          		<td>Mendefinisikan sebuah tombol berisi perintah yang bisa user tekan</td>
          	</tr>

			<tr>
          		<td><code>&lt;datalist&gt;</code></td>
          		<td>Menspesifikasikan daftar dari opsi yang sudah dipersiapkan.</td>
          	</tr>

          	<tr>
          		<td><code>&lt;details&gt;</code></td>
          		<td>Mendefinisikan detail tambahan yang bisa ditampilkan dan disembunyikan oleh user</td>
          	</tr>

          	<tr>
          		<td><code>&lt;dialog&gt;</code></td>
          		<td>Mendefinisikan dialog box</td>
          	</tr>

          	<tr>
          		<td><code>&lt;embed&gt;</code></td>
          		<td>Mendefinisikan kontainer dari sumber eksternal</td>
          	</tr>

          	<tr>
          		<td><code>&lt;figure&gt;</code></td>
          		<td>Konten yang bebas dari depedensi.</td>
          	</tr>

          	<tr>
          		<td><code>&lt;header&gt;</code></td>
          		<td>Mendefinisikan header suatu konten</td>
          	</tr>

          	<tr>
          		<td><code>&lt;nav&gt;</code></td>
          		<td>Mendefinisikan navigasi</td>
          	</tr>

          	<tr>
          		<td><code>&lt;progress&gt;</code></td>
          		<td>Merepresentasikan kemajuan dari sebuah task</td>
          	</tr>

          	<tr>
          		<td><code>&lt;section&gt;</code></td>
          		<td>Mendefinisikan section dalam konten</td>
          	</tr>
          </tbody>
	</table>
{% endblock %}